  "keywords": [
    "PostgreSQL",
    "template",
    "literal",
    "tag",
    "query"
  ],
  "author": "Andrea Giammarchi",
  "license": "ISC",
  "devDependencies": {
    "ascjs": "^5.0.1",
    "c8": "^7.10.0",
    "coveralls": "^3.1.1",
    "pg": "^8.7.1"
  },
  "dependencies": {
    "plain-tag": "^0.1.3",
    "static-params": "^0.2.1"
  }
}
